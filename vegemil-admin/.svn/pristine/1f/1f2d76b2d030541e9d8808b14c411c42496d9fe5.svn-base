				var wheel = e.originalEvent.wheelDelta;
				if(wheel>0){
				//스크롤 올릴때
				$('.counter').countUp();
				} else if(wheel < 0) {
				//스크롤 내릴때
				$('.counter').countUp();
				}
				if(e.keyCode =='37' || e.keyCode =='38') {
					$('.counter').countUp();
				}
			});
			
